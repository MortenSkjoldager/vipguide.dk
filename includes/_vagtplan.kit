<script type="text/javascript" src="/scripts/min/app.min.js"></script>
<ul id="feed" class="feed collapsible collection" data-collapsible="accordion">
    <script id="feed-template" type="text/x-handlebars-template">
        <li>
            <div class="collapsible-header collection-item avatar">
                <img src="http://vipguide.dk/assets/img/{{toLowerCase cells.Firma}}.png" alt="{{cells.Firma}}" class="circle">
                <span class="title">{{cells.Firma}}</span>
                <p>{{cells.Type}} kl. {{cells.Afgang}}
                    <br>{{cells.Antal}} personer
                    <br>
                    <a href="/hold.html">Hold {{cells.Hold}}</a></p>
                {{#if cells.Drejebog}}
                <a target="_blank" href="{{cells.Drejebog}}" class="secondary-content">Drejebog</a> {{/if}}
            </div>
            <div class="collapsible-body">
                <div class="flex-container">
                        {{#if cells.Moedetidspunkt}}
                        <div class="flex-item">
                        <h6>M√∏detidspunkt</h6>
                        M√∏d kl. <u>{{cells.Moedetidspunkt}}</u>, men kom gerne f√∏r og f√• en üç∫
                        </div>
                        {{/if}}

                        {{#if cells.Kontaktperson}}
                        <div class="flex-item">
                        <h6>Kontaktperson</h6>
                        <p>{{cells.Kontaktperson}}</p>
                        </div>
                        {{/if}}

                        {{#if cells.Mobil}}
                        <div class="flex-item">
                        <h6>Mobil</h6>
                        <p>{{cells.Mobil}}</p>
                        </div>
                        {{/if}} 

                        {{#if cells.Kommentar}}
                        <div class="flex-item">
                        <h6>Kommentarer</h6>
                        <p>{{cells.Kommentar}}</p>
                        </div>
                        {{/if}}
                    
                    
                        {{#if cells.Afhentning}}
                        <div class="flex-item">
                        <h6>Afhentning</h6>
                        <p>{{cells.Afhentning}}</p>
                        </div>
                        {{/if}} 

                        {{#if cells.Tur}}
                        <div class="flex-item">
                        <h6>Tur</h6>
                        <p>{{cells.Tur}}</p>
                        </div>
                        {{/if}}
                    </div>
                </div>
            </div>
        </li>
    </script>
</ul>
